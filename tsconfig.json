{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["node_modules/**/*", "dist/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@lib/*": ["src/lib/*"],
      "@blog-images/*": ["src/blog-images/*"]
    },
  }
}
